[run]
source = .
branch = True
omit = 
    */test/*
    */tests/*
    setup.py
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == "__main__":
    pass
    raise ImportError
    raise AssertionError

[html]
directory = coverage_html